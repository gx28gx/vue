                width 100%
                height 130px
                position relative
                z-index 1
                display flex
                li
                  width 105.714px
                  font-size 14px
                  text-align center
                  height 130px
                  flex-shrink 0
                  position relative
                  a
                    display inline-block
                    box-sizing border-box
                    height 130px
                    padding 0 2px
                    color #7e8c8d
                    text-decoration none
                    .bottom_s1
                      width 100%
                      img
                        width 100%
                        box-sizing border-box
                        padding 0 5%
                        display block
                    .bottom_s2
                      width 100%
                      display inline-block
                      color #ff0000
                      font-size 14px
                      margin 5px 0
                      padding 0
                      font-weight normal
                      vertical-align baseline
                    .bottom_s3
                      width 100%
                      display inline-block
                      color #999999
                      font-size 12px
