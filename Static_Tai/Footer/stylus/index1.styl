@import "mixins.styl"
body
  margin 0
  padding 0
  width 100%
  height 100%
  background #fff
  box-sizing border-box
  .app_top
    max-width 640px
    margin 0 auto
    background rgba(61,61,61,1)
    color #fff
    position relative
    z-index 102
    .top_wrap
      position relative
      .wrap_span
        display inline-block
        width 5%
        margin-right 2%
        vertical-align middle
        position absolute
        top 20%
        left 2%
        .wrap_img
          height 100%
          width 100%
          vertical-align middle
      .wrap_div
        display block
        .wrap_a
          display block
          text-decoration none
          /*-webkit-tap-highlight-color rgba(0,0,0,0)*/
          .xia_zai
            vertical-align middle
            display block
            width 100%

  #app
    max-width 640px
    margin auto
    .content
      height 86px
      >div
        width 100%
        height 100%
        display none
        &.on
          display block
          .header1
            z-index 100
            height 86px
            .header_wrap
              max-width 640px
              margin auto
            .up
              padding-left 10px
              padding-bottom 5px
              padding-right 10px
              padding-top 5px
              .up_wrap
                padding 8px 0
                background #ffffff
                text-align center
                display -webkit-box
                -webkit-box-align center
                width 100%
                .wrap_city
                  margin-right 20px
                  position relative
                  font-size 14px
                  float left
                  a
                    color #333
                    text-decoration none
                    -webkit-tap-highlight-color rgba(0,0,0,0)
                    span
                      color #898989
                    &.s3
                      font-size 13px
                    i
                      position absolute
                      top 6px
                      right -12px
                      border-width 4px
                      font-size 0
                      line-height 0
                      border-style solid solid dotted
                      border-color #898989 transparent transparent
                      font-style normal
                .wrap_search
                  display block
                  position relative
                  -webkit-box-flex 1
                  a
                    color #333333
                    text-decoration none
                    -webkit-tap-highlight-color rgba(0,0,0,0)
                    input
                      width 100%
                      border 0
                      height 25px
                      background #e9e9e9
                      border-radius 4px
                      color #bcbcbc
                      text-indent 10px
                      font-size 13px
                      outline 0
                    .wrap_icon
                      width 11px
                      height 11px
                      position absolute
                      right 5px
                      top 0
                      bottom 0
                      margin auto 0
                      background-position 0 0
                      background-image url("../images/search.png")
                      background-size 11px auto

                .wrap_new
                  display block
                  color #333333
                  text-decoration none
                  -webkit-tap-highlight-color rgba(0,0,0,0)
                  img
                    width 25px
                    margin-left 10px
                    border 0
                    vertical-align middle
            .down
              width 100%
              height 35px
              min-width 320px
              .down_wrap
                overflow-x auto
                overflow-y hidden
                .wrap_list
                  width 140%
                  ul
                    white-space nowrap
                    display flex
                    list-style none
                    .list_li
                      width 74.6429px
                      float left
                      list-style none
                      display list-item
                      text-align -webkit-match-parent
                      a
                        display block
                        width 100%
                        height 100%
                        line-height 36px
                        font-size 14px
                        text-align center
                        color #666
                        text-decoration none
                        -webkit-tap-highlight-color rgba(0,0,0,0)
                        >span
                          position relative
                          >i
                            position absolute
                            bottom 1px
                            left -10%
                            width 32px
                            height 2px
                            font-style normal
                        .list_span
                          color #459d36
                          display inline-block
                          .list_shouye
                            color #459d36
                            display inline-block
                          .list_i
                            background #459d36

          .swiper-slide
            width 375px
            height auto
            img
              width 100%
              height 100%
          .d1_d3
